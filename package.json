{
    "name": "puck-bash-commander",
    "displayName": "Puck - Bash Commander",
    "description": "part of the Puck suite of AI development tools",
    "version": "0.0.1",
    "engines": {
      "vscode": "^1.77.0"
    },
    "categories": [
      "Other"
    ],
    "contributes": {
      "configuration": {
        "title": "puck Settings",
        "properties": {
          "puck.apikey": {
            "type": "password",
            "default": "sk-",
            "description": "API key for the OpenAI GPT service."
          },
          "puck.temperature": {
            "type": "number",
            "default": 0.7,
            "description": "Default temperature to use in queries"
          },
          "puck.maxTokens": {
            "type": "number",
            "default": 2048,
            "description": "Default max tokens to use in queries"
          }
        }
      },
      "commands": [
        {
          "command": "puck.bashCommander",
          "title": "puck: Bash Commander"
        }
      ]
    },
    "main": "./out/extension.js",
    "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./",
      "pretest": "npm run compile && npm run lint",
      "lint": "eslint src --ext ts",
      "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
      "@types/blessed": "^0.1.21",
      "@types/glob": "^8.1.0",
      "@types/jest": "^29.5.1",
      "@types/markdown-it": "^12.2.3",
      "@types/marked": "^4.0.8",
      "@types/mocha": "^10.0.1",
      "@types/node": "^16.18.25",
      "@types/react-dom": "^18.2.1",
      "@types/sha256": "^0.2.0",
      "@types/shelljs": "^0.8.12",
      "@types/vscode": "^1.77.0",
      "@types/xmlserializer": "^0.6.3",
      "@typescript-eslint/eslint-plugin": "^5.59.2",
      "@typescript-eslint/parser": "^5.59.2",
      "@vscode/test-electron": "^2.3.0",
      "eslint": "^8.36.0",
      "eslint-plugin-react": "^7.32.2",
      "glob": "^8.1.0",
      "jest": "^29.5.0",
      "mocha": "^10.2.0",
      "ts-jest": "^29.1.0",
      "typescript": "^4.9.5"
    },
    "dependencies": {
      "@ant-design/icons": "^5.0.1",
      "@jupyterlab/apputils": "^3.6.3",
      "@jupyterlab/notebook": "^3.6.3",
      "@jupyterlab/services": "^6.6.3",
      "@lumino/disposable": "^2.1.0",
      "@monaco-editor/react": "^4.5.0",
      "@svgdotjs/svg.js": "^3.1.2",
      "@vscode/webview-ui-toolkit": "^1.2.2",
      "antd": "^5.4.6",
      "axios": "^1.3.6",
      "blessed": "^0.1.81",
      "cheerio": "^1.0.0-rc.12",
      "diff2html": "^3.4.35",
      "markdown-it": "^13.0.1",
      "marked": "^4.3.0",
      "mongodb-client-encryption": "^2.7.1",
      "node-pty": "^0.10.1",
      "ohm-js": "^17.1.0",
      "os-dns-native": "^1.2.0",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "sha256": "^0.2.0",
      "shelljs": "^0.8.5",
      "sqlite3": "^5.1.6",
      "tiny-hashes": "^1.0.1",
      "vscode-debugadapter": "^1.51.0",
      "vscode-languageclient": "^8.1.0",
      "xmlserializer": "^0.6.1"
    }
  }
  